{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "perx-whistler-services": {
      "projectType": "library",
      "root": "projects/perx-whistler-services",
      "sourceRoot": "projects/perx-whistler-services/src",
      "prefix": "ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/perx-whistler-services/tsconfig.lib.json",
            "project": "projects/perx-whistler-services/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/perx-whistler-services/tsconfig.lib.json",
              "projects/perx-whistler-services/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "perx-whistler-services"
}
