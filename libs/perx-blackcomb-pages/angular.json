{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "perx-blackcomb-pages": {
      "projectType": "library",
      "root": "projects/perx-blackcomb-pages",
      "sourceRoot": "projects/perx-blackcomb-pages/src",
      "prefix": "perx-blackcomb-pages",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/perx-blackcomb-pages/tsconfig.lib.json",
            "project": "projects/perx-blackcomb-pages/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/perx-blackcomb-pages/src/test.ts",
            "tsConfig": "projects/perx-blackcomb-pages/tsconfig.spec.json",
            "karmaConfig": "projects/perx-blackcomb-pages/karma.conf.js",
            "codeCoverage": false
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/perx-blackcomb-pages/tsconfig.lib.json",
              "projects/perx-blackcomb-pages/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "perx-blackcomb-pages",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}