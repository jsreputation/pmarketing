{
  "name": "@perx/chart",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:lib": "ng build",
    "clean": "rm -rf ./dist ./node_modules",
    "test": "jest",
    "lint": "stylelint \"projects/**/*.*css\" && eslint \"projects/**/*.ts\"",
    "lint:fix": "stylelint \"projects/**/*.*css\" --fix",
    "e2e~": "ng e2e",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@briebug/jest-schematic": "^2.1.1",
    "@swimlane/ngx-charts": "^12.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular-devkit/build-ng-packagr": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/google.visualization": "0.0.48",
    "@types/jest": "24.0.25",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.19.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-no-focused-test": "^0.5.0",
    "typescript": "~3.4.3"
  },
  "main": "dist/perx-chart/bundles/perx-chart.umd.js",
  "module": "dist/perx-chart/fesm5/perx-chart.js",
  "es2015": "dist/perx-chart/fesm2015/perx-chart.js",
  "esm5": "dist/perx-chart/esm5/perx-chart.js",
  "esm2015": "dist/perx-chart/esm2015/perx-chart.js",
  "fesm5": "dist/perx-chart/fesm5/perx-chart.js",
  "fesm2015": "dist/perx-chart/fesm2015/perx-chart.js",
  "typings": "dist/perx-chart/perx-chart.d.ts",
  "metadata": "dist/perx-chart/perx-chart.metadata.json",
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "projects"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup-jest.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/projects/perx-chart/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  }
}