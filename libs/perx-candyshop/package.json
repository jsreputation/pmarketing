{
  "name": "@perxtech/candyshop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:lib": "ng build && scss-bundle",
    "build-mylib": "ng build && npm run build-styles && npm run cp-assets",
    "copy-assets": "cpx -u \"projects/perx-candyshop/src/assets\" \"dist/perx-candyshop/assets\"",
    "clean": "rm -rf ./dist ./node_modules",
    "test": "jest --maxWorkers=50%",
    "lint": "stylelint \"projects/**/*.*css\" && eslint \"projects/**/*.ts\"",
    "lint:fix": "stylelint \"projects/**/*.*css\" --fix && eslint \"projects/**/*.ts\" --fix",
    "e2e~": "ng e2e",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "lodash.isequal": "^4.5.0",
    "mat-color-picker": "^1.4.3",
    "moment": "^2.24.0",
    "ngx-color-picker": "^8.1.0",
    "ngx-material-timepicker": "^4.0.0",
    "rxjs": "~6.4.0",
    "saturn-datepicker": "^7.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular-devkit/build-ng-packagr": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jest": "25.1.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "~2.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "~2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "codelyzer": "^5.0.0",
    "cpx": "^1.5.0",
    "eslint": "~6.4.0",
    "jest": "25.1.0",
    "jest-preset-angular": "8.0.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "scss-bundle": "^3.0.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.19.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "~3.4.3"
  },
  "main": "dist/perx-candyshop/bundles/perxtech-candyshop.umd.js",
  "module": "dist/perx-candyshop/fesm5/perxtech-candyshop.js",
  "es2015": "dist/perx-candyshop/fesm2015/perxtech-candyshop.js",
  "esm5": "dist/perx-candyshop/esm5/perxtech-candyshop.js",
  "esm2015": "dist/perx-candyshop/esm2015/perxtech-candyshop.js",
  "fesm5": "dist/perx-candyshop/fesm5/perxtech-candyshop.js",
  "fesm2015": "dist/perx-candyshop/fesm2015/perxtech-candyshop.js",
  "typings": "dist/perx-candyshop/perxtech-candyshop.d.ts",
  "metadata": "dist/perx-candyshop/perxtech-candyshop.metadata.json",
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "projects"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/projects/setup-jest.ts"
    ],
    "moduleNameMapper": {},
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/projects/perx-candyshop/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  }
}