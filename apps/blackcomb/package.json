{
  "name": "@perx/blackcomb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config && ng serve --host 0.0.0.0",
    "start-zh": "DEFAULT_LANG=zh npm run config && ng serve",
    "start-ru": "DEFAULT_LANG=ru npm run config && ng serve",
    "build": "npm run config && ng build",
    "build:blackcomb:prod": "PRODUCTION=true npm run config && ng build --prod",
    "config": "ts-node --project tsconfig.node.json set-env.ts",
    "test": "ng test",
    "lint": "ng lint && stylelint \"src/**/*.*css\" && eslint \"./src/*/*.ts\"",
    "lint:fix": "ng lint --fix && stylelint \"src/**/*.*css\" --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "~8.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@perx/core": "^0.0.0",
    "@sentry/browser": "^5.10.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
