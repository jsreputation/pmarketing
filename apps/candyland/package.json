{
  "name": "@perx/candyland",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=4000 ../../node_modules/@angular/cli/bin/ng",
    "start": "npm run config && ng serve --host 0.0.0.0",
    "build": "npm run config && ng build",
    "build:candyland:prod": "PRODUCTION=true npm run config && npm run ng-high-memory -- build --prod",
    "config": "ts-node --project tsconfig.node.json set-env.ts",
    "clean": "rm -rf ./dist ./node_modules",
    "test": "ng test",
    "lint": "ng lint && stylelint src/**/*.*css",
    "lint:fix": "ng lint --fix && stylelint \"src/**/*.*css\" --fix",
    "styleLint": "stylelint src/**/*.*css",
    "pree2e": "webdriver-manager update",
    "e2e-pro": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/pwa": "^0.803.3",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "~8.0.0",
    "@perx/core": "^0.0.0",
    "@perx/chart": "^0.0.0",
    "angular2-jsonapi": "^7.1.2",
    "hammerjs": "^2.0.8",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.transform": "^4.6.0",
    "mat-color-picker": "^1.4.3",
    "moment": "^2.24.0",
    "ngx-auth": "^5.0.0",
    "ngx-color-picker": "^8.1.0",
    "ngx-material-timepicker": "^4.0.0",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "~6.4.0",
    "saturn-datepicker": "^7.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/chai": "^4.2.0",
    "@types/cucumber": "^4.0.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.15",
    "@types/node": "~10.14.13",
    "@types/protractor-browser-logs": "^1.0.9",
    "chai": "^4.2.0",
    "codelyzer": "^5.0.0",
    "cucumber": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "protractor-browser-logs": "^1.0.456",
    "protractor-console": "^3.0.0",
    "protractor-cucumber-framework": "^6.1.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "~3.4.3"
  }
}
