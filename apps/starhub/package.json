{
  "name": "@perxtech/starhub",
  "version": "0.6.23",
  "scripts": {
    "ng": "ng",
    "start": "env-cmd -f ./env/starhub.staging.dev npm run config && ng serve --host 0.0.0.0",
    "start:prod": "env-cmd -f ./env/starhub.production.dev npm run config && ng serve --host 0.0.0.0",
    "build": "npm run config && ng build",
    "build:starhub:production": "env-cmd -f ./env/starhub.production npm run config && ng build --prod",
    "build:starhub:staging": "env-cmd -f ./env/starhub.staging npm run config && ng build --configuration staging",
    "config": "ts-node --project tsconfig.node.json set-env.ts",
    "clean": "rm -rf ./dist ./node_modules",
    "test": "jest --config=jest.config.json",
    "lint": "stylelint \"src/**/*.*css\" && ng lint",
    "lint:fix": "stylelint \"src/**/*.*css\" --fix && eslint \"./src/**/*.ts\" --fix",
    "test:watch": "jest --watch --config=jest.config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "~8.0.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@perxtech/blackcomb-pages": "^0.0.0",
    "@perxtech/core": "^0.0.0",
    "ngx-auth": "^5.0.0",
    "ngx-infinite-scroll": "^8.0.1",
    "ngx-multi-line-ellipsis": "^1.0.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "colors": "~1.0.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jest": "25.1.2",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "~2.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "~2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "codelyzer": "^5.0.0",
    "eslint": "~6.4.0",
    "jest": "25.1.0",
    "jest-preset-angular": "8.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-no-focused-test": "^0.5.0",
    "typescript": "~3.4.3"
  }
}
