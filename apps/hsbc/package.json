{
  "name": "@perx/hsbc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config && ng serve --host 0.0.0.0",
    "start:xmas": "SOURCE_TYPE=hsbc-xmas CAMPAIGN_ID=331 npm run config && ng serve --host 0.0.0.0",
    "start:collect2": "SOURCE_TYPE=hsbc-collect2 npm run config && ng serve --host 0.0.0.0",
    "build": "npm run config && ng build",
    "build:hsbc:prod": "PRODUCTION=true npm run config && ng build --prod",
    "build:hsbc-xmas:prod": "SOURCETYPE=hsbc-xmas PRODUCTION=true npm run config && ng build --prod",
    "config": "ts-node --project tsconfig.node.json set-env.ts",
    "clean": "rm -rf ./dist ./node_modules",
    "test": "ng test",
    "lint": "stylelint \"src/**/*.*css\" && eslint \"./src/*/*.ts\"",
    "lint:fix": "stylelint \"src/**/*.*css\" --fix && eslint \"./src/*/*.ts\" --fix",
    "e2e": "ng e2e"
  },
  "private": true,
